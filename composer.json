{
  "name": "stanbic/kenya-sdk",
  "description": "Stanbic Bank Kenya PHP SDK (unofficial, community-maintained)",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=8.1",
    "psr/http-client": "^1.0",
    "psr/http-message": "^1.0",
    "psr/log": "^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5",
    "vimeo/psalm": "^5.24",
    "squizlabs/php_codesniffer": "^3.10",
    "symfony/var-dumper": "^6.4",
    "guzzlehttp/guzzle": "^7.8",
    "phpstan/phpstan": "^1.11"
  },
  "autoload": {
    "psr-4": {
      "Stanbic\\SDK\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Stanbic\\SDK\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit",
    "lint": "phpcs --standard=PSR12 src tests",
    "psalm": "psalm --no-cache",
    "phpstan": "phpstan analyse src tests"
  }
}
